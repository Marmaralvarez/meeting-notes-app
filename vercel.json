{
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    }
  ],
  "routes": [
    {
      "src": "/reset-password",
      "dest": "/reset-password.html"
    },
    {
      "src": "/app",
      "dest": "/app.html"
    }
  ],
  "headers": [
    {
      "source": "/app.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/login.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/reset-password.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}
