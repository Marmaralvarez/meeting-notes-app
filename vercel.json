{
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    }
  ],
  "redirects": [
    {
      "source": "/app",
      "destination": "/app.html",
      "permanent": false
    },
    {
      "source": "/reset-password",
      "destination": "/reset-password.html", 
      "permanent": false
    },
    {
      "source": "/login",
      "destination": "/login.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}
